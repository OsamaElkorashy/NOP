@model FoxNetSoft.Plugin.Misc.CMSManager.Models.Admin.PageModel
@{
    Layout = "_AdminLayout";
    //page title
    ViewBag.Title = @T("Admin.FoxNetSoft.CMSManager.Page.AddNew").Text;

    Html.SetActiveMenuItemSystemName("FoxNetSoft.CMSManager.Pages");
}
<form asp-controller="CMSPageAdmin" asp-action="Create" method="post" id="cmspage-form">
    <div class="content-header clearfix">
        <h1 class="pull-left">
            @T("Admin.FoxNetSoft.CMSManager.Page.AddNew")
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a asp-action="List">@T("Admin.FoxNetSoft.CMSManager.Page.BackToList")</a>
            </small>
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
            <button type="submit" name="save-continue" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.SaveContinue")
            </button>
        </div>
    </div>
    @await Html.PartialAsync("~/Plugins/FoxNetSoft.CMSManager/Views/Admin/PageAdmin/_CreateOrUpdate.cshtml", Model)
</form>
